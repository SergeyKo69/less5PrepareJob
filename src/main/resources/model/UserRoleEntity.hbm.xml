<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.kogut.model.UserRoleEntity" table="UserRole" schema="mybatis">
        <id name="id">
            <column name="id" sql-type="varchar(45)" length="45"/>
        </id>
        <property name="userId">
            <column name="userId" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="roleId">
            <column name="roleId" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>