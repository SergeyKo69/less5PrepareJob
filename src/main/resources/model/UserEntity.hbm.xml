<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.kogut.model.UserEntity" table="User" schema="mybatis">
        <id name="id">
            <column name="id" sql-type="varchar(45)" length="45"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="login">
            <column name="login" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="eMail">
            <column name="eMail" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>